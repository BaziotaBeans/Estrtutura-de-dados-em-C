BIBLIOTECA=Biblioteca/
DOMINO_OBJECTO=$(BIBLIOTECA)domino.o
DOMINO_IMPLEMENTACAO=$(BIBLIOTECA)domino.c
DOMINO=domino.o
CLIENTE=Main.c

exec: $(DOMINO_OBJECTO)
	gcc $(CLIENTE) $(DOMINO_OBJECTO) -I $(BIBLIOTECA) -o exec

$(DOMINO_OBJECTO): $(DOMINO_IMPLEMENTACAO)
	gcc -c $(DOMINO_IMPLEMENTACAO)
	mv $(DOMINO) $(BIBLIOTECA)
