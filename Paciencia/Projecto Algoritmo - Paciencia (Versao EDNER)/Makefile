BIBLIOTECA=Biblioteca/
PACIENCIA_OBJECTO=$(BIBLIOTECA)paciencia.o
PACIENCIA_IMPLEMENTACAO=$(BIBLIOTECA)paciencia.c
PACIENCIA=paciencia.o
CLIENTE=Main.c

exec: $(PACIENCIA_OBJECTO)
	gcc $(CLIENTE) $(PACIENCIA_OBJECTO) -I $(BIBLIOTECA) -o exec

$(PACIENCIA_OBJECTO): $(PACIENCIA_IMPLEMENTACAO)
	gcc -c $(PACIENCIA_IMPLEMENTACAO)
	mv $(PACIENCIA) $(BIBLIOTECA) 





  

