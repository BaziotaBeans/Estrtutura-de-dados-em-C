BIBLIOTECA=Biblioteca/
PROCESSO_OBJECTO=$(BIBLIOTECA)processo.o
PROCESSO_IMPLEMENTACAO=$(BIBLIOTECA)processo.c
PROCESSO=processo.o
CLIENTE=Main.c

exec: $(PROCESSO_OBJECTO)
	gcc $(CLIENTE) $(PROCESSO_OBJECTO) -I $(BIBLIOTECA) -o exec

$(PROCESSO_OBJECTO): $(PROCESSO_IMPLEMENTACAO)
	gcc -c $(PROCESSO_IMPLEMENTACAO)
	mv $(PROCESSO) $(BIBLIOTECA)

